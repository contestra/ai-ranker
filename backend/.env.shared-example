# Example .env for using shared Upstash Redis instance
# Copy this to backend/.env and fill in your values

# Database - Fly.io will auto-configure this
# DATABASE_URL=

# Shared Upstash Redis (same as your other app)
UPSTASH_REDIS_REST_URL=https://direct-pup-22413.upstash.io
UPSTASH_REDIS_REST_TOKEN=your_token_here

# LLM API Keys
OPENAI_API_KEY=sk-...
GOOGLE_API_KEY=AIza...
ANTHROPIC_API_KEY=sk-ant-...

# LangSmith (optional but recommended)
LANGCHAIN_API_KEY=ls__...
LANGCHAIN_TRACING_V2=true
LANGCHAIN_PROJECT=ai-ranker

# Server settings
HOST=0.0.0.0
PORT=8080
ENVIRONMENT=production